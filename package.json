{
  "name": "stargate-grpc-node-client",
  "version": "0.0.0",
  "description": "A NodeJS gRPC client for Stargate.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "compile": "rm -rf lib && tsc",
    "prestart": "npm run gen && npm run compile",
    "start": "node lib/index.js",
    "trace": "GRPC_TRACE=all GRPC_VERBOSITY=debug node lib/index.js",
    "gen": "grpc_tools_node_protoc --proto_path=proto --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --js_out=import_style=commonjs,binary:src/proto --ts_out=src/proto proto/query.proto proto/stargate.proto"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/google-protobuf": "^3.15.5",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "grpc-tools": "^1.11.2",
    "jest": "^27.1.1",
    "protoc-gen-ts": "^0.4.0",
    "testcontainers": "^7.20.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "axios": "^0.21.4",
    "google-protobuf": "^3.18.0-rc.2"
  }
}
